{
    "name": "frittenkeez/e-takeaway",
    "description": "API integration for e-takeaway (http://api.e-takeaway.com/V1/Documentation/)",
    "type": "library",
    "keywords": ["etakeaway", "e-takeaway"],
    "homepage": "https://github.com/FrittenKeeZ/e-takeaway",
    "license": "MIT",
    "authors": [
        {
            "name": "Frederik Sauer",
            "email": "fritten.keez@gmail.com",
            "homepage": "http://frebs.dk",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.4.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "squizlabs/php_codesniffer": "~2.3"
    },
    "autoload": {
        "psr-4": {
            "Etakeaway\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Etakeaway\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    }
}
